{
  "compilerOptions": {
    "module": "amd",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "baseUrl": "/",
    "paths": {
      "assets/*": [
        "src/assets/*"
      ]
    }
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}